{
  "name": "finance-tracker-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "vitest --run src/e2e",
    "test:unit": "vitest --run --exclude src/e2e/**",
    "test:performance": "tsx src/scripts/performance-test.ts",
    "seed": "tsx src/scripts/seed-data.ts",
    "generate-insights": "tsx src/scripts/generate-insights.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@supabase/supabase-js": "^2.38.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "node-cron": "^3.0.3",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "axios": "^1.6.2",
    "eslint": "^8.55.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vitest": "^4.0.9"
  }
}
